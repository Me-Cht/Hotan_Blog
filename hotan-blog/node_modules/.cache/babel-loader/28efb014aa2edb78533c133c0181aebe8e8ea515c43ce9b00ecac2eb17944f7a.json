{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'FruitQuery',\n  data() {\n    return {\n      queryForm: {\n        name: '',\n        category: '',\n        minPrice: '',\n        maxPrice: ''\n      },\n      fruitList: []\n    };\n  },\n  methods: {\n    doQuery() {\n      axios.post('http://localhost:8088/api/product/search', this.queryForm, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(res => {\n        this.fruitList = res.data;\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","queryForm","category","minPrice","maxPrice","fruitList","methods","doQuery","post","headers","then","res"],"sources":["src/views/fruit/search/SearchView.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>水果商品查询</h2>\n    <el-form :inline=\"true\" :model=\"queryForm\" class=\"query-form\">\n      <el-form-item label=\"名称\">\n        <el-input v-model=\"queryForm.name\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"分类\">\n        <el-select v-model=\"queryForm.category\" placeholder=\"请选择\">\n          <el-option label=\"全部\" value=\"\"></el-option>\n          <el-option label=\"水果\" value=\"水果\"></el-option>\n          <el-option label=\"蔬菜\" value=\"蔬菜\"></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"价格\">\n        <el-input v-model=\"queryForm.minPrice\" placeholder=\"最低价\" style=\"width: 100px;\"></el-input>~\n        <el-input v-model=\"queryForm.maxPrice\" placeholder=\"最高价\" style=\"width: 100px;\"></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"doQuery\">查询</el-button>\n      </el-form-item>\n    </el-form>\n\n    <el-table :data=\"fruitList\" style=\"width: 100%\">\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\n      <el-table-column prop=\"name\" label=\"名称\"></el-table-column>\n      <el-table-column prop=\"category\" label=\"分类\"></el-table-column>\n      <el-table-column prop=\"description\" label=\"描述\"></el-table-column>\n      <el-table-column prop=\"price\" label=\"价格\" width=\"120\"></el-table-column>\n    </el-table>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'FruitQuery',\n  data() {\n    return {\n      queryForm: {\n        name: '',\n        category: '',\n        minPrice: '',\n        maxPrice: ''\n      },\n      fruitList: []\n    }\n  },\n  methods: {\n    doQuery() {\n      axios.post('http://localhost:8088/api/product/search',\n          this.queryForm,\n\n          {\n            headers: {\n              'Content-Type': 'application/json'\n            }\n          }\n      ).then(res => {\n        this.fruitList = res.data\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.query-form {\n  margin-bottom: 20px;\n}\n</style>\n"],"mappings":"AAkCA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;QACAF,IAAA;QACAG,QAAA;QACAC,QAAA;QACAC,QAAA;MACA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,QAAA;MACAT,KAAA,CAAAU,IAAA,6CACA,KAAAP,SAAA,EAEA;QACAQ,OAAA;UACA;QACA;MACA,EACA,CAAAC,IAAA,CAAAC,GAAA;QACA,KAAAN,SAAA,GAAAM,GAAA,CAAAX,IAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}