   name: '',\n        price: ''\n      }\n    }\n  },\n  mounted() {\n    const id = this.$route.params.id;\n    axios.get(`http://hotan.site:8088/api/product/findById/${id}`)\n        .then(response => {\n          this.form = response.data\n          console.log(\"获取成功！\")\n        })\n        .catch(error => {\n          console.log('error!', error)\n        })\n  },\n  methods: {\n    submitForm() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          axios.put(`http://hotan.site:8088/api/product/update/${this.form.id}`, this.form)\n              .then(response => {\n                console.log('修改成功！')\n                this.$router.push('/')\n              })\n              .catch(error => {\n                console.log('error!', error)\n              })\n        } else {\n          console.log('error submit!!')\n          return false\n        }\n      })\n    }\n  }\n}\n</script>\n<style scoped>\n.edit-fruit-view {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n  background-color: #f2f2f2;\n  border: 1px solid #d3d3d3;\n  border-radius: 5px;\n  box-shadow: 0 0 5px #d3d3d3;\n}\n\n</style>\n"],"mappings":";AAiBA,OAAAA,KAAA;AAEA;EACAC,IAAA;EAGAC,KAAA;IACA;MACAC,IAAA;QACAC,EAAA;QACAH,IAAA;QACAI,KAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,MAAAF,EAAA,QAAAG,MAAA,CAAAC,MAAA,CAAAJ,EAAA;IACAJ,KAAA,CAAAS,GAAA,gDAAAL,EAAA,IACAM,IAAA,CAAAC,QAAA;MACA,KAAAR,IAAA,GAAAQ,QAAA,CAAAT,IAAA;MACAU,OAAA,CAAAC,GAAA;IACA,GACAC,KAAA,CAAAC,KAAA;MACAH,OAAA,CAAAC,GAAA,WAAAE,KAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAC,KAAA,CAAAf,IAAA,CAAAgB,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACApB,KAAA,CAAAqB,GAAA,mDAAAlB,IAAA,CAAAC,EAAA,SAAAD,IAAA,EACAO,IAAA,CAAAC,QAAA;YACAC,OAAA,CAAAC,GAAA;YACA,KAAAS,OAAA,CAAAC,IAAA;UACA,GACAT,KAAA,CAAAC,KAAA;YACAH,OAAA,CAAAC,GAAA,WAAAE,KAAA;UACA;QACA;UACAH,OAAA,CAAAC,GAAA;UACA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}